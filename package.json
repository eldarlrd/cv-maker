{
  "name": "cv-maker",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "keywords": [
    "resume builder",
    "cv maker"
  ],
  "description": "A Resume Builder",
  "repository": "github:eldarlrd/cv-maker",
  "homepage": "https://eldarlrd.is-a.dev/cv-maker",
  "author": "Eldar Pashazade <eldarlrd@pm.me> (https://eldarlrd.is-a.dev)",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "bundle": "tsc -b && vite build",
    "preview": "vite preview",
    "deployer": "bun bundle && gh-pages -b prod -m 'build(prod): deployment' -d dist",
    "lint": "eslint src",
    "style": "stylelint 'src/**/*.{css,pcss,scss,sass,less,styl}'",
    "pretty": "prettier '{src,__{tests,mocks,snapshots}__}/**/*.{ts,tsx}' --write",
    "format": "bun lint --fix && bun style --fix && bun pretty",
    "cut": "knip --fix --cache"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-stylus/standard"
    ],
    "rules": {
      "media-feature-range-notation": "prefix",
      "no-invalid-position-declaration": null,
      "declaration-property-value-no-unknown": null,
      "declaration-property-value-keyword-no-deprecated": null
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "experimentalTernaries": true
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "modern-normalize": "^3.0.1",
    "nanoid": "^5.1.6",
    "non.geist": "^1.0.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-to-print": "^3.2.0",
    "stylus": "^0.64.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "knip": "^5.69.1",
    "prettier": "^3.6.2",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-stylus": "^1.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0"
  }
}
